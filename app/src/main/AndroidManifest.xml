        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SzokerMobil">
        <activity
            android:name=".MainActivity"
            android:exported="true">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity android:name=".SzakmunkasLogin"
            android:theme="@style/Theme.Design.Light.NoActionBar"
            tools:ignore="Instantiatable"/>

        <activity android:name=".MegrendeloLogin"
            android:theme="@style/Theme.Design.Light.NoActionBar"
            tools:ignore="Instantiatable"/>

        <activity android:name=".Login_utvalaszto"
            android:theme="@style/Theme.Design.Light.NoActionBar"
            tools:ignore="Instantiatable" />

        <activity android:name=".Register"
            android:theme="@style/Theme.Design.Light.NoActionBar"/>

        <activity android:name=".Megrendelok"
            android:theme="@style/Theme.Design.Light.NoActionBar"/>

        <activity android:name=".Szakmunkasok"
            android:theme="@style/Theme.Design.Light.NoActionBar"/>





    </application>

</manifest>